<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="repository.UserMapper">

    <!--회원 정보 변경 - 비밀번호, 이름, 전화번호-->
    <update id="modification" parameterType="domain.Users">
        UPDATE footballMatching.user
        SET password = #{password}, name = #{name}, phoneNumber = #{phoneNumber}
        WHERE id = #{id};
    </update>

</mapper>